   "source": [
    "class Solution:\n",
    "    def climbStairs(self, n):\n",
    "        result = [1, 2]\n",
    "        for i in range(2, n):\n",
    "            result.append(result[i-1] + result[i-2])\n",
    "        return result[n-1]\n",
    "        \n",
    "sol = Solution()\n",
    "sol.climbStairs(1)"
   ]
