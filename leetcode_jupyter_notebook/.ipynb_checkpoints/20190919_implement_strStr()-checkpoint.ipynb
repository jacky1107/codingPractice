   "source": [
    "class Solution:\n",
    "    def strStr(self, haystack, needle) -> int:\n",
    "        if needle == \"\":\n",
    "            return 0\n",
    "        return haystack.index(needle) if needle in haystack else -1\n",
    "        \n",
    "        \n",
    "haystack = \"a\"\n",
    "needle = \"a\"\n",
    "sol = Solution()\n",
    "sol.strStr(haystack, needle)"
   ]
